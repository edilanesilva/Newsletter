@model Usuario
@{
    ViewData["Title"] = "Home Page";
}

<h2>Cadastro de Newsletter</h2>

<form asp-action="Cadastro" method="POST">
    <P>
        <label asp-for="Nome">Nome:</label>
        <input asp-for="Nome">
    </P>
    <P>
        <label asp-for="Email">Email:</label>
        <input asp-for="Email">
    </P>
    <P>
        <label asp-for="Genero">Genero:</label>
        <input type="radio" asp-for="Genero" Value="M">Masculino
        <input type="radio" asp-for="Genero" Value="F">Feminino
    </P>
    <input type="submit" class="btn-primary" value="Cadastrar">
</form>


