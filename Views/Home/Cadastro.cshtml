@model List<Usuario>


    <style>
        table
        { width: 100%;
        text-align: center;
        padding-bottom: 2em;
        margin-bottom: 2em;
        }
    </style>
    <h2>Usuario  Cadastrado com suceso</h2>

    <table style="margin-bottom: 2em; padding-bottom:2em">
        <thead>
            <tr>
                <th>Nome</th>
                <th>Email</th>
                <th>Genero</th>
            </tr>
        </thead>    
        <tbody>
            @foreach (Usuario u in Model)
            {
                <tr>
                    <td>@u.Nome</td>
                    <td>@u.Email</td>
                    <td>@u.Genero</td>
                </tr>    
            }
        </tbody>
    
    </table>

    <a href="/" class="btn-primary">Voltar ao cadastro</a>
